# Performance Monitoring Configuration
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1
REACT_APP_PERFORMANCE_SLOW_THRESHOLD=16
REACT_APP_PERFORMANCE_MAX_METRICS=1000
REACT_APP_PERFORMANCE_ENABLE_MEMORY_TRACKING=true
REACT_APP_PERFORMANCE_ENABLE_INTERACTION_TRACKING=true

# API Configuration
REACT_APP_API_BASE_URL=/api
REACT_APP_API_TIMEOUT=5000

# Logging Configuration
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_PERFORMANCE_LOGGING=true

# Security Configuration
REACT_APP_API_KEY=your-api-key-here  # Required for production 

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password-here  # Required for production
REDIS_TLS_ENABLED=false  # Set to true for production

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100  # requests per window 